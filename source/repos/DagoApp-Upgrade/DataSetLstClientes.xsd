                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
  c.IdCliente AS ID,
  c.Nombre,
  c.NOTarjeta AS Tarjeta,
  c.FechaVencimientoTarjeta,
  c.Pin AS PIN,
  c.Telefono,
  c.CorreoElectronico AS Correo,
  (CASE
    WHEN c.TipoCliente = 1 THEN 'Interes'
    ELSE 'Cuota Fija'
  END)
  AS TipoCiente,
  e.Nombre AS Empresa,
  co.Nombre AS Cobrador
FROM Clientes c
INNER JOIN Empresas e
  ON c.IdEmpresa = e.IdEmpresa
INNER JOIN Cobradores co
  ON c.IdCobrador = co.IdCobrador</CommandText>
